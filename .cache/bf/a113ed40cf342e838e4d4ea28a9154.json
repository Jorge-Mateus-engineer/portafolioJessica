 --main-color-shade1: #daf6f2;\r\n  --main-color-transparent: rgba(72, 209, 191, 0.5);\r\n  --accent-color: #ff6468;\r\n  --accent-color-shade1: #ffa2a4;\r\n  --accent-tint1: #331415;\r\n  --greys-color: #30304c;\r\n  --greys-color-shade1: rgba(152, 152, 166, 0.5);\r\n  --greys-color-transparent: rgba(48, 48, 76, 0.5);\r\n  --new-color: #faaa8c;\r\n}\r\n\r\n* {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nhtml {\r\n  font-size: 62.5%;\r\n  overflow-x: hidden;\r\n  font-family: \"Balsamiq Sans\", cursive;\r\n}\r\n\r\nhtml {\r\n  background-color: var(--main-color-shade1);\r\n}\r\n\r\nhtml::-webkit-scrollbar {\r\n  width: 1rem;\r\n  height: 0;\r\n}\r\n\r\nhtml::-webkit-scrollbar-track {\r\n  background-color: var(--main-color-shade1);\r\n}\r\n\r\nhtml::-webkit-scrollbar-thumb {\r\n  background-color: var(--accent-color);\r\n  border-radius: 6px;\r\n  outline: 1px transparent var(--greys-color);\r\n}\r\n\r\n/* Navigation */\r\n\r\n.nav-container {\r\n  height: 8rem;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  background-color: #fb9772;\r\n}\r\n\r\n.nav-icon {\r\n  height: 6rem;\r\n  width: 6rem;\r\n  margin-left: 4rem;\r\n}\r\n\r\n.nav-name {\r\n  font-size: 3rem;\r\n  padding-left: 3rem;\r\n  letter-spacing: 3px;\r\n}\r\n\r\n.left-nav {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.nav-list {\r\n  display: flex;\r\n  list-style: none;\r\n  align-items: center;\r\n  gap: 2rem;\r\n  margin-right: 6rem;\r\n}\r\n\r\n.nav-item {\r\n  font-size: 2rem;\r\n}\r\n\r\n.nav-item a {\r\n  transition: all 0.3s;\r\n  padding: 1rem;\r\n}\r\n\r\n.nav-item a:link,\r\n.nav-item a:visited {\r\n  text-decoration: none;\r\n  /* color: var(--main-color-shade1); */\r\n  color: #803234;\r\n}\r\n\r\n.nav-item a:hover,\r\n.nav-item a:active {\r\n  text-decoration: none;\r\n  background-color: var(--accent-color);\r\n  border-radius: 5px;\r\n}\r\n\r\n.nav-logo {\r\n  fill: var(--main-color-shade1);\r\n  transition: all 0.3s;\r\n}\r\n\r\n.nav-logo:hover {\r\n  background-color: var(--accent-color);\r\n  border-radius: 5px;\r\n}\r\n\r\n/* Projects */\r\n\r\n.projects-container {\r\n  padding: 2rem;\r\n  height: calc(100vh - 8rem);\r\n}\r\n\r\n.projects {\r\n  height: 100%;\r\n  width: 100%;\r\n  overflow: clip;\r\n  display: flex;\r\n  flex-direction: row;\r\n}\r\n\r\n.project {\r\n  position: relative;\r\n  display: inline-block;\r\n  overflow: clip;\r\n  height: 100%;\r\n  width: 90rem;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.titulo-diseño,\r\n.titulo-ilustracion,\r\n.titulo-fotografia {\r\n  position: absolute;\r\n  font-size: 5rem;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  left: 2rem;\r\n  color: var(--greys-color);\r\n  background-color: var(--main-color-shade1);\r\n  display: inline-block;\r\n  z-index: 4;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.project > img {\r\n  height: 100%;\r\n  transform: scale(1.3);\r\n  position: absolute;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.p1 {\r\n  margin-right: -20%;\r\n}\r\n\r\n.p2 {\r\n  margin-right: -20%;\r\n  border-radius: 34% 0% 0% 38% / 41% 10% 10% 46%;\r\n  width: 100rem;\r\n  z-index: 2;\r\n}\r\n\r\n.p3 {\r\n  z-index: 3;\r\n  border-radius: 34% 0% 0% 38% / 41% 10% 10% 46%;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"060e3f75a5fa0a071a6f4c55620aabd4","cacheData":{"env":{}}}